@model Mde.Model.DataModel.MD_Account
@{
    ViewBag.Title = "Account";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

@section style{
    <style>
        .spanClass {
            width: 200px;
            word-wrap: break-word;
            word-break: normal;
            display: block;
            font-weight: 100;
        }
    </style>
}

@section script{
    <script type="text/javascript">
        $(function () {
            //查询事件
            $("#btn_Search").click(function () {
                RefreshGrid();
            });
            //InitOrganizationData($("#SOrganization"));
            var url = "@Url.Action("GetCompanyComboboxData", "Shipment")" + "?NeedAll=true";
            InitCombobox($("#SCompany"), url, "");

            @*url = "@Url.Action("GetDictionaryComboxData","Base")" + "?Category=DistributionChannel&NeedAll=true";
            InitCombobox($("#SDistribution"), url, "");*@

            InitGrid();

            $("#btnClose").click(function () {
                $("#UpdateDialog").dialog("close");
            });
        });

        //刷新数据
        function RefreshGrid() {
            var queryData = {
                CompanyCode: $("#SCompany").combobox("getValue"),
                Account: $("#SAccount").val()
            };
            $("#grid").datagrid({
                pageNumber: 1,
                url: "@Url.Action("GetAccountDataList", "Master")",
                queryParams: queryData
            });
            CheckButtonRole("@Url.Action("GetRoleFromCache","Base")");
        }

        function InitGrid() {
            var queryData = {
                CompanyCode: $("#SCompany").combobox("getValue"),
                Account: $("#SAccount").val()
            };
            $("#grid").datagrid({
                fit: true,
                //fitColumns: true,
                rownumbers: false,
                border: false,
                singleSelect: true,
                pagination: true,
                pageSize: 30,
                pageNumber: 1,
                method: 'post',
                striped: true,
                url: '@Url.Action("GetAccountDataList", "Master")',
                idField: 'AccountNumber',
                queryParams: queryData,
                columns:
                    [[

                        { field: 'AccountName', title: '@Resources.Language.txt_AccountName' },
                        { field: 'AccountNumber', title: '@Resources.Language.txt_AccountNumber' },
                        { field: 'SalesOrg', title: '@Resources.Language.txt_SalesOrg' },
                        { field: 'SalesGroup', title: '@Resources.Language.txt_SalesGroup' },
                        { field: 'SubChannel', title: '@Resources.Language.txt_SubChannel' },
                        { field: 'SalesRoute', title: '@Resources.Language.txt_SalesRoute' },
                        { field: 'ShippingCondition', title: '@Resources.Language.txt_ShippingCondition' },
                        { field: 'InvoiceCalendar', title: '@Resources.Language.txt_InvoiceCalendar' },
                        {
                            field: 'Active', title: '@Resources.Language.txt_Status', width: 50,
                            formatter: function (value, row, index) {
                                if (value == 1) {
                                    return "Y";
                                }
                                if (value == 0) {
                                    return "<font color='red'>N</font>";
                                }
                            }
                        }
                    ]
                    ],
                toolbar: [{
                    id: 'btnView_1',
                    text: '@Resources.Language.btn_View',
                    iconCls: 'icon-search',
                    handler: function () {
                        ShowDialog();
                    }
                }],
                rowStyler: function (index, row) {
                    if (row.DeleteFlag == 1) {
                        return "color:Red";
                    }
                }
            });
            CheckButtonRole("@Url.Action("GetRoleFromCache","Base")");
        }

        //显示弹出层
        function ShowDialog() {
            var title;

            //判断是否选中行
            var row = $("#grid").datagrid("getSelected");
            if (!row) {
                $.messager.alert("@Resources.Message.msg_AlertTitle", "@Resources.Message.msg_SelectRecord");
                return;
            }
            else {
                title = "@Resources.Language.btn_View";
                //Account Detail
                $("#AccountName").html(row.AccountName);
                $("#AccountName1").html(row.AccountName1);
                $("#AccountName2").html(row.AccountName2);
                $("#Owner").html(row.Owner);
                $("#RecordType").html(row.RecordType);
                $("#AccountGroup").html(row.AccountGroup);
                $("#AccountParent").html(row.AccountParent);
                $("#AccountNumber").html(row.AccountNumber);
                $("#ERPAccountNumber").html(row.ERPAccountNumber);
                $("#TaxNumber").html(row.TaxNumber);
                $("#VATNumber").html(row.VATNumber);
                $("#SuspressionCode").html(row.SuspressionCode);
                $("#SuppressionReason").html(row.SuppressionReason);
                $("#CompanyCode").html(row.CompanyCode);
                $("#SalesOrg").html(row.SalesOrg);
                $("#SalesOrgCode").html(row.SalesOrgCode);
                $("#SalesDistrict").html(row.SalesDistrict);
                $("#SalesGroup").html(row.SalesGroup);
                $("#SalesOffice").html(row.SalesOffice);
                $("#VisitStartDate").html(row.VisitStartDate);
                $("#SubChannel").html(row.SubChannel);
                $("#SalesRoute").html(row.SalesRoute);
                $("#ShippingCondition").html(row.ShippingCondition);
                $("#InvoiceCalendar").html(row.InvoiceCalendar);
                $("#DeliveryDate").html(row.DeliveryDate);
                $("#DeliveryDays").html(row.DeliveryDays);
                $("#DiscountIndicator").html(row.DiscountIndicator);
                $("#BillTo").html(row.BillTo);
                $("[name=IsPush]:checkbox").attr("checked", row.IsPush);
                $("[name=RedIndecator]:checkbox").attr("checked", row.RedIndecator);

