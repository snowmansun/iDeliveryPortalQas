                $("#StreetNumber").html(row.StreetNumber);
                $("#Street").html(row.Street);
                $("#Street4").html(row.Street4);
                $("#Street5").html(row.Street5);

                //Financial
                $("#PaymentTerms").html(row.PaymentTerms);
                $("#CreditLimit").html(row.CreditLimit);
                $("#OpenItemAgingStart").html(row.OpenItemAgingStart);
                $("#CreditDays").html(row.CreditDays);
                $("#TotalOpenBalance").html(row.TotalOpenBalance);
                $("#CreditExposure").html(row.CreditExposure);
            }

            $("#UpdateDialog").dialog({ title: title });
            $("#UpdateDialog").dialog('open');
        }

        //绑定combobox
        function InitCombobox(Control, Url, DefaultValue) {
            Control.combobox({
                url: Url,
                valueField: "value",
                textField: "name",
                editable: false,
                onLoadSuccess: function () { //加载完成后,设置选中第一项
                    var val = $(this).combobox("getData");
                    if (DefaultValue == "") {
                        if (val.length > 0)
                            $(this).combobox("select", val[0].value);
                    }
                    else {
                        $(this).combobox("select", DefaultValue);
                    }
                }
            });
        }

        function InitOrganizationData(control) {
            control.combotree({
                url: "@Url.Action("GetOrganizationTreeData", "Master")",
                method: 'get',
                editable: false,
                //multiple: true,
                cascadeCheck: true,
                lines: true,
                onLoadSuccess: function (node, data) {
                    $(this).tree("collapseAll");
                }
            });
        }

        function DetailShow(divId) {
            $("#" + divId).toggle();
        }
    </script>
}

